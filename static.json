{
  "https_only": true,
  "root": "build/",
  "proxies": {
    "/api-new/": {
      "origin": "${EMBER_API_DOCS_URL}"
    }
  },
  "headers": {
    "/images/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/javascripts/**": {
      "Cache-Control": "max-age=31536000"
    },
    "/stylesheets/**": {
      "Cache-Control": "max-age=31536000"
    }
  },
  "redirects": {
    "/api-new": {
      "url": "/api-new/"
    },
    "/sw.js": {
      "url": "https://ember-api-docs-staging-herokuapp-com.global.ssl.fastly.net/sw.js"
    },
    "/sw-registration.js": {
      "url": "https://ember-api-docs-staging-herokuapp-com.global.ssl.fastly.net/sw-registration.js"
    }
  }
}
